    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/resize-observer-browser": "^0.1.7",
    "babel-loader": "^8.2.4",
    "husky": "^7.0.4",
    "prettier": "2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.0.0",
    "size-limit": "^7.0.8",
    "storybook-dark-mode": "^1.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@mdx-js/react": "^2.1.1",
    "framer-motion": "^6.2.8",
    "theme-ui": "^0.14.2"
  }
