  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gedesurya125/surya-ui",
  "author": "gedesurya125@gmail.com",
  "module": "dist/surya-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/surya-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/surya-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-info": "^5.3.21",
